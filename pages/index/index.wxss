.header{
  position: relative;
}
.header-input {
  line-height: 72rpx;
  padding: 0 36rpx;
  margin: 22rpx 36rpx 14rpx 36rpx;
  border: 1rpx solid #d9dde1;
  border-radius: 36rpx;
  /* 水平居中 */
  text-align: center; 
  font-size: 26rpx;
}

.header-input-placeholder{
  color: rgba(133, 133, 133, 0.60);
}

.header-input-icon{
  position: absolute;
  top: 4rpx;
  transform: translateX(55rpx);
  color: rgba(133, 133, 133, 0.60);
}

.navigator{
  display: flex;
  line-height: 112rpx;
  margin-bottom: 6rpx;
}
.navigator-item{
  position: relative;
  flex: 1;
  text-align: center;   /* 水平居中 */
  vertical-align: middle;   /* 垂直居中 */
  font-size: 32rpx;
  color: #2B333B;
}
.navigator-image{
  position: relative;
  top: 7rpx;
  width: 36rpx;
  height: 36rpx;
}
.navigator-active{
  position: absolute;
  width: 32rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background: #F01414;
  left: 90rpx;
  bottom: 20rpx;
}
.swiper{
  height: 232rpx;
}
.swiper-item{
  box-sizing: border-box;
}
.swiper-image{
  width: 714rpx;
  height: 232rpx;
  border-radius: 12rpx;
}

